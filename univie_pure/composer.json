{
    "name": "univie/univie_pure",
    "type": "typo3-cms-extension",
    "description": "REST api to Vienna pure system",
    "homepage": "https://univie.ac.at",
    "license": "GPL-2.0+",
    "keywords": [
        "TYPO3 CMS",
        "Pure",
        "u:cris",
        "Elsevier"
    ],
    "support": {
        "issues": ""
    },
    "require": {
      "typo3/cms-core": "^12"
    },
    "autoload": {
        "psr-4": {
            "Univie\\UniviePure\\": "Classes/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor/",
        "bin-dir": ".Build/bin/"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "univie_pure"
        }
    }
}
